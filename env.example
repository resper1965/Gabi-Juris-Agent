# Configurações do Servidor
PORT=3000
NODE_ENV=development

# APIs Externas (adicione suas chaves de API aqui)
DUCKDUCKGO_API_KEY=your_duckduckgo_api_key_here
OPENAI_API_KEY=your_openai_api_key_here
WEATHER_API_KEY=your_weather_api_key_here

# Configurações do Magic Chat da 21st.dev
MAGIC_CHAT_BASE_URL=https://api.21st.dev
MAGIC_CHAT_API_KEY=your_magic_chat_api_key_here
MAGIC_CHAT_PROJECT_ID=your_project_id_here
MAGIC_CHAT_ENVIRONMENT=development

# Configurações de Banco de Dados
DATABASE_URL=postgresql://username:password@localhost:5432/database_name
DATABASE_HOST=localhost
DATABASE_PORT=5432
DATABASE_NAME=database_name
DATABASE_USER=username
DATABASE_PASSWORD=password

# Configurações de WebSocket
WEBSOCKET_PORT=8080
WEBSOCKET_HOST=localhost

# Configurações de Segurança
JWT_SECRET=your_jwt_secret_here
CORS_ORIGIN=http://localhost:3000

# Configurações de Log
LOG_LEVEL=info
LOG_FILE=logs/app.log 